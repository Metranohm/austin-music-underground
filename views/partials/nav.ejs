<% if (!user) { %>
  <script defer src="https://accounts.google.com/gsi/client"></script>
<% } %>
</head>
<body>
<nav>
  <img src="https://img.icons8.com/ios-filled/500/underground-music.png" alt="logo" id="logo">
  <a href="/venues/new" <%- title === "Add Venue" ? "class='active'" : '' %>>Add Venue</a>
  <a href="/venues" <%- title === "All Venues" ? "class='active'" : '' %>>All Venues</a>
  <a href="/profiles"></a>
  <% if (user) { %>
    <a href="/auth/logout">Logout</a>
  <% } else { %>
    <div 
      id="g_id_onload"
      data-client_id="<%= googleClientID %>"
      data-login_uri="/auth/google"
      data-auto_prompt="false"
    ></div>
    <div 
      class="g_id_signin"
      data-type="standard"
      data-size="large"
      data-theme="outline"
      data-text="sign_in_with"
      data-shape="rectangular"
      data-logo_alignment="left"
    ></div>
  <% } %>
</nav>